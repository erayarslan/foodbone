{
  "name": "foodbone",
  "version": "0.0.1",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "cookies",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "offline_enabled": true
}
